{
  "name": "SGBus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn ios",
    "ios": "react-native run-ios --simulator='iPhone Xs Max'",
    "ios2": "react-native run-ios --simulator='iPhone 4s'",
    "android": "react-native run-android",
    "android:run-release": "react-native run-android --variant=release",
    "android:release": "cd android && ./gradlew bundleRelease && cd ..",
    "generate:stubs": "yarn generate:bus-lta && yarn generate:bus-onemap && yarn generate:bus-searchable",
    "generate:bus-searchable": "babel-node ./scripts/bus/searchable.js --presets @babel/env",
    "generate:bus-lta": "babel-node ./scripts/bus/lta.js --presets @babel/env",
    "generate:bus-onemap": "babel-node ./scripts/bus/onemap.js --presets @babel/env",
    "generate:secret": "babel-node ./scripts/app-secret.js --presets @babel/env",
    "test": "jest",
    "lint": "eslint .",
    "npm-clear": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean",
    "npm-reinstall": "yarn npm-clear && yarn",
    "pod-clear": "cd ios && pod deintegrate && pod clean",
    "pod-reinstall": "cd ios && yarn pod-clear && pod install",
    "link-all": "react-native link react-native-gesture-handler && react-native link react-native-svg && react-native link react-native-geolocation-service && react-native link @mapbox/react-native-mapbox-gl && react-native link lottie-ios && react-native link lottie-react-native",
    "unlink-all": "react-native unlink react-native-gesture-handler && react-native unlink react-native-svg && react-native unlink react-native-geolocation-service && react-native unlink @mapbox/react-native-mapbox-gl && react-native link lottie-ios && react-native unlink lottie-react-native",
    "flow": "flow"
  },
  "dependencies": {
    "@mapbox/react-native-mapbox-gl": "https://github.com/nitaliano/react-native-mapbox-gl.git",
    "@react-native-community/netinfo": "^4.1.3",
    "axios": "^0.18.0",
    "d3-shape": "^1.3.5",
    "date-fns": "^1.30.1",
    "i18next": "^17.0.7",
    "lodash": "^4.17.11",
    "lottie-react-native": "2.6.1",
    "react": "16.8.6",
    "react-i18next": "^10.11.4",
    "react-native": "0.60.5",
    "react-native-geolocation-service": "^3.0.0",
    "react-native-gesture-handler": "^1.0.16",
    "react-native-svg": "^9.3.6",
    "react-navigation": "^3.5.1",
    "react-navigation-redux-helpers": "^3.0.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-filesystem-storage": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "why-did-you-update": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@mapbox/polyline": "^1.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "flow-bin": "0.93.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.8.6",
    "reactotron-react-native": "^3.2.1",
    "reactotron-redux": "^3.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
